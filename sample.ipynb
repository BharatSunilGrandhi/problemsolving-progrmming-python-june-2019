    "# Function to define char count \n",
    "def charcount(filepath):\n",
    "    with open(filepath, 'r') as f:\n",
    "        char=0\n",
    "        for i in f:\n",
    "            char=char+len(i)\n",
    "        print(char)\n",
    "filepath='Data Files/data.txt'\n",
    "charcount(filepath)"
